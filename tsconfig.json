{
  "compilerOptions": {
    "target": "es2017",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/ui/dashboard/FormCandidate/FormCandidate.jsx", "app/api/upload-mongo-image/route.js", "app/ui/dashboard/FormVacancy/FormVacancy.jsx", "app/dashboard/vacancy/add/page.jsx", "app/dashboard/tasks/page.jsx", "app/api/auth/[...nextauth]/route.js",  ],
  "exclude": [
    "node_modules"
  ]
}
